    "pem-jwk": "^2.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/fast-crc32c": "^2.0.1",
    "@types/jest": "^29.5.7",
    "@types/jwk-to-pem": "^2.0.2",
