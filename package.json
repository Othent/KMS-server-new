    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.17.0 <22.0.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
