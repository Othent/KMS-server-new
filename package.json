{
  "name": "kms-server-new",
  "version": "1.0.0",
  "description": "Server for KMS interactions.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20.5.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "bun run prettier && tsup src/index.ts --format cjs,esm --dts",
    "dev": "bun run ./src/index.ts --watch",
    "heroku-postbuild": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest tests/ --forceExit",
    "prettier": "prettier --write ."
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Othent/kms-server-new.git"
  },
  "author": "Lorimer Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Othent/kms-server-new/issues"
  },
  "homepage": "https://github.com/Othent/kms-server-new#readme",
  "dependencies": {
    "@google-cloud/kms": "^4.0.1",
    "arbundles": "^0.11.0",
    "axios": "^1.6.0",
    "base64-js": "^1.5.1",
    "base64url": "^3.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jwks-rsa": "^3.1.0",
    "mongodb": "^6.3.0",
    "multer": "^1.4.5-lts.1",
    "pem-jwk": "^2.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/fast-crc32c": "^2.0.1",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/jwk-to-pem": "^2.0.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/pem-jwk": "^2.0.1",
    "bun-types": "latest",
    "google-auth-library": "^9.11.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}
