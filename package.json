  },
  "homepage": "https://github.com/Othent/kms-server-new#readme",
  "dependencies": {
    "@google-cloud/kms": "^4.0.1",
    "arbundles": "^0.11.0",
    "axios": "^1.6.0",
    "base64-js": "^1.5.1",
    "base64url": "^3.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
