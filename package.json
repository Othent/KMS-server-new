{
  "name": "kms-server-new",
  "version": "1.0.0",
  "description": "Server for KMS interactions.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20.5.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run prettier && rm -rf ./dist && rollup -c && dts-bundle-generator src/index.ts -o dist/index.d.ts --no-banner",
    "dev": "npm run build && node dist/index.js",
    "heroku-postbuild": "rm -rf ./dist && rollup -c && dts-bundle-generator src/index.ts -o dist/index.d.ts --no-banner",
    "test": "jest tests/tests/ --coverage",
    "prettier": "prettier --write ."
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Othent/kms-server-new.git"
  },
  "author": "Lorimer Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Othent/kms-server-new/issues"
  },
  "homepage": "https://github.com/Othent/kms-server-new#readme",
  "dependencies": {
    "@google-cloud/kms": "^4.0.1",
    "arweave-mnemonic-keys": "^0.0.9",
    "axios": "^1.6.0",
    "base64-js": "^1.5.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "multer": "^1.4.5-lts.1",
    "pem-jwk": "^2.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/fast-crc32c": "^2.0.1",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/jwk-to-pem": "^2.0.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/pem-jwk": "^2.0.1",
    "bun-types": "latest",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "rollup": "^4.3.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "tslib": "^2.6.2",
    "dts-bundle-generator": "^8.1.2"
  }
}
